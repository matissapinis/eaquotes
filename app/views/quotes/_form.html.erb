<%= form_with(model: @quote) do |f| %>
  <%= f.label :content %>
  <%= f.text_area :content %>

  <%= f.label :attribution %>
  <%= f.text_field :attribution %>

  <%= f.label :source %>
  <%= f.text_field :source %>

  <%= f.label :source_link %>
  <%= f.text_field :source_link %>

  <%= f.label :comment %>
  <%= f.text_area :comment %>

  <%= f.submit %>
<% end %>
